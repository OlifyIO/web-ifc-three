{
  "name": "example",
  "version": "0.0.100",
  "author": "IFC.js",
  "license": "Mozilla Public License Version 2.0",
  "description": "This library is the implementation of [web-ifc](https://github.com/tomvandig/web-ifc) for [THREE.js](https://github.com/mrdoob/three.js/). This is the official IFCLoader of Three.js.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OlifyIO/web-ifc-three.git"
  },
  "keywords": [
    "bim",
    "openbim",
    "ifc",
    "architecture",
    "engineering",
    "construction",
    "aec"
  ],
  "bugs": {
    "url": "https://github.com/OlifyIO/web-ifc-three/issues"
  },
  "homepage": "https://github.com/OlifyIO/web-ifc-three#readme",
  "scripts": {
    "build": "rollup -c && npm run copy-files",
    "watch": "rollup -c -w",
    "copy-files": "cpy ../node_modules/web-ifc/web-ifc.wasm . && cpy ../node_modules/web-ifc/web-ifc-mt.wasm . && cpy ../web-ifc-three/dist/IFCWorker.js . && cpy ../web-ifc-three/dist/IFCWorker.js.map ."
  },
  "dependencies": {
    "stats.js": "^0.17.0",
    "three": "^0.135.0",
    "web-ifc": "^0.0.32",
    "web-ifc-three": "0.0.99"
  },
  "devDependencies": {
    "cpy-cli": "^3.1.1"
  }
}
